import pandas as pd
import matplotlib.pyplot as plt

# Sample data
data = {
    "Year": [2019, 2020, 2021, 2022, 2023],
    "Sales": [200, 250, 300, 280, 350]
}

df = pd.DataFrame(data)

# Line plot
plt.plot(df["Year"], df["Sales"], marker='o')
plt.xlabel("Year")
plt.ylabel("Sales")
plt.title("Year-wise Sales")
plt.grid(True)
plt.show()
